<uexplugins>
    <plugin
        className="org.zywx.wbpalmstar.plugin.ueximage.EUExImage"
        uexName="uexImage">
        <method
            name="openPicker"
            params="min|max|quality|usePng|detailedInfo"
            type="1"/>
        <method
            name="openBrowser"
            params="enableGrid|startOnGrid"
            type="1"/>
        <method
            name="openCropper"
            params="src|startOnGrid|quality|usePng|detailedInfo"
            type="1"/>
        <method
            name="saveToPhotoAlbum"
            params="localPath|extraInfo"
            type="1"/>
        <method
            name="clearOutputImages"
            params="localPath|extraInfo"
            type="1"/>

    </plugin>
</uexplugins>