<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.zywx.wbpalmstar.plugin.ueximage"
    android:installLocation="internalOnly"
    android:versionCode="300"
    android:versionName="3.0" >

    <application>
        <activity
            android:name="org.zywx.wbpalmstar.engine.LoadingActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="standard"
            android:screenOrientation="portrait"
            android:theme="@style/browser_loading_theme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlbumListActivity"
            android:theme="@style/plugin_uex_image_appTheme" >
        </activity>
        <activity
            android:name=".PictureGridActivity"
            android:theme="@style/plugin_uex_image_appTheme" >
        </activity>
        <activity
            android:name=".ImagePreviewActivity"
            android:theme="@style/plugin_uex_image_appTheme" >
        </activity>
        <activity android:name="com.soundcloud.android.crop.CropImageActivity"
            android:theme="@style/CustomCropTheme" />
    </application>
</manifest>
